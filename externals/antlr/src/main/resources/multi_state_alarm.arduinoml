application multiStateAlarm

# Declaring bricks
sensor button: 9
actuator led: 12
actuator buzzer: 11

# Declaring states

led {
    led <= HIGH
    buzzer <= LOW
    button is HIGH => off
}

buzzer {
    led <= LOW
    buzzer <= HIGH
    button is HIGH => led
}

-> off {
    led <= LOW
    buzzer <= LOW
    button is HIGH => buzzer
}